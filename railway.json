{
    "$schema": "https://railway.com/railway.schema.json",
    "build": {
      "builder": "DOCKERFILE",
      "watchPatterns":[
        "/app/*.py",
        "/app/**/*.py",
        "/dockerfile",
        "/pyproject.toml"
      ]
      },
    "deploy": {
      "restartPolicyType": "ON_FAILURE"
      }
  }